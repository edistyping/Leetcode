class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int i = 0;
        for(i = 0; i < nums.size(); i++){
           if(nums[i] >= target){
               return i; 
           }
           if(i == nums.size() - 1 && nums[i] <= target){ // checking last index
               return i+1; 
           }
       } 
        return i; 
    }
};

